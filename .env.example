# Environment variables for Deep-Seek-temporal-reasoner
# Copy this file to .env and fill in your actual values

# DeepSeek API Configuration
DEEPSEEK_API_KEY=your-deepseek-api-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_MODEL=deepseek-chat

# Application Configuration
APP_NAME=Deep-Seek-temporal-reasoner
APP_VERSION=1.0.0
DEBUG=false

# Server Configuration
HOST=localhost
PORT=8000

# Temporal Processing Configuration
MAX_PROMPT_LENGTH=4096
ENABLE_PAST_ANALYSIS=true
ENABLE_PRESENT_ANALYSIS=true
ENABLE_FUTURE_ANALYSIS=true

# Logging Configuration
LOG_LEVEL=INFO